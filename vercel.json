{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "src/app/api/process-invoices/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/process-package/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/**/*": {
      "maxDuration": 60
    }
  },
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_WEB_SDK_VERSION": "1.8.0"
  }
}